-- Table: public.managerSignUp

-- DROP TABLE public.managerSignUp;

CREATE TABLE public.managerSignUp
(
  "teamName"  text,
  onePerson bit(1),
  team bit(1),
  either bit(1),
  "teamRank" text,
  "playerType" text,
  "experience" text,
  "preferred_Champion_Team" text,
  lookingForMembers bit(1),
  lookingForCoach bit(1),
  username text
)
WITH (
  OIDS=FALSE
);
ALTER TABLE public.characteristics
  OWNER TO postgres;
  
